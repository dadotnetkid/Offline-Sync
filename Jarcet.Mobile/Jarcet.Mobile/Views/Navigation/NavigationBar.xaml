<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Jarcet.Mobile.Views.Navigation.NavigationBar"
             xmlns:pages="clr-namespace:Jarcet.Mobile.Views.Navigation"
             xmlns:qoute="clr-namespace:Jarcet.Mobile.Views.Qoute;assembly=Jarcet.Mobile"
             >

    <MasterDetailPage.Master>
        <pages:NavigationBarMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail >
        <NavigationPage >
            <x:Arguments>
                <qoute:Qoutes/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>